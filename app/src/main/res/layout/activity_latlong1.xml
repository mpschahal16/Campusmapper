<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mapping"
    tools:context="com.example.deepa.campusmapper.Latlong1Activity">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="398dp"
        android:layout_height="122dp"
        android:gravity="center"
        android:text="Calculate Latitude Longitude"
        android:textColor="@color/colorPrimary"
        android:textSize="30dp"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="86dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        tools:layout_editor_absoluteY="81dp" />

    <TextView
        android:id="@+id/textView2"
        android:padding="5dp"
        android:layout_width="381dp"
        android:layout_height="73dp"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        android:textSize="30dp"
        android:text="Processing Long......."
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/start1"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="39dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteX="-15dp"
        app:layout_constraintHorizontal_bias="0.526"
        tools:layout_editor_absoluteY="277dp" />

    <TextView
        android:id="@+id/textView3"
        android:padding="5dp"
        android:layout_width="399dp"
        android:layout_height="68dp"
        android:gravity="center"
        android:textColor="@color/colorPrimary"
        android:textSize="30dp"
        android:text="Processing Lat......."
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.437"
        app:layout_constraintHorizontal_bias="0.571"
        tools:layout_editor_absoluteY="190dp"
        tools:layout_editor_absoluteX="-13dp" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/class1"
        android:background="#FFFF"
        android:hint="Classroom No."
        android:textAlignment="center"
        android:textColorHint="@color/colorPrimaryDark"
        android:textColor="@color/colorPrimary"
        android:padding="4dp"
        android:layout_weight="0.17"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/editText1"
        tools:layout_constraintBottom_creator="1" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:id="@+id/editText1"
        android:background="#FFFF"
        android:hint="Altitude"
        android:textAlignment="center"
        android:textColorHint="@color/colorPrimaryDark"
        android:textColor="@color/colorPrimary"
        android:padding="4dp"
        android:layout_weight="0.17"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="49dp"
        app:layout_constraintHorizontal_bias="0.555" />

    <Button
        android:id="@+id/start1"
        android:layout_width="144dp"
        android:layout_height="51dp"
        android:text="START"
        android:onClick="onClick"
        android:layout_marginBottom="81dp"
        android:background="@color/colorPrimary"
        android:textColor="#FFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="42dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.333" />

    <Button
        android:id="@+id/stop"
        android:layout_width="128dp"
        android:layout_height="51dp"
        android:text="STOP"
        android:onClick="onClick"
        app:layout_constraintLeft_toRightOf="@+id/start1"
        android:layout_marginLeft="44dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:background="@color/colorPrimary"
        android:textColor="#FFF"
        app:layout_constraintVertical_bias="0.051" />

    <Button
        android:id="@+id/button10"
        android:layout_width="174dp"
        android:layout_height="52dp"
        android:text="BACK"
        android:onClick="back"
        android:background="@color/colorPrimary"
        android:textColor="#FFF"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintVertical_bias="0.911" />


</android.support.constraint.ConstraintLayout>
