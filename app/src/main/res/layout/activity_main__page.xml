<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:background="@drawable/mapping"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start"
    tools:context="com.example.deepa.campusmapper.Main_Page">

    <include
        layout="@layout/app_bar_main__page"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <android.support.design.widget.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/nav_header_main__page"
        app:menu="@menu/activity_main__page_drawer" >

    </android.support.design.widget.NavigationView>


    <android.support.constraint.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="1">

        <Button
            android:id="@+id/button9"
            android:layout_width="370dp"
            android:layout_height="241dp"
            android:onClick="onClick"
            android:layout_weight="0.10"
            android:background="@drawable/colg"
            card_view:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="76dp"
            android:layout_marginLeft="23dp"
            card_view:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="23dp"
            card_view:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="23dp"
            card_view:layout_constraintBottom_toBottomOf="@+id/button10"
            android:layout_marginEnd="23dp"
            tools:layout_constraintLeft_creator="1" />

        <ImageView
            android:id="@+id/button10"
            android:layout_width="43dp"
            android:layout_height="46dp"
            android:textSize="22sp"
            android:layout_gravity="center"
            android:textStyle="italic"
            android:onClick="onClick"
            android:src="@drawable/home"
            android:textAlignment="center"
            android:layout_weight="0.10"
            android:layout_marginBottom="30dp"
            card_view:layout_constraintBottom_toTopOf="@+id/button13"
            android:layout_marginLeft="67dp"
            card_view:layout_constraintLeft_toLeftOf="parent"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintHorizontal_bias="0.0"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="67dp"
            android:layout_marginEnd="67dp"
            tools:layout_constraintLeft_creator="1" />

        <ImageView
            android:id="@+id/button11"
            android:layout_width="43dp"
            android:layout_height="42dp"
            android:textSize="22sp"
            android:layout_gravity="center"
            android:textStyle="italic"
            android:paddingTop="0dp"
            android:src="@drawable/exit"
            android:onClick="onClick"
            android:textAlignment="center"
            android:layout_weight="0.10"
            android:layout_marginLeft="64dp"
            card_view:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="64dp"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="2dp"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="64dp"
            android:layout_marginEnd="64dp"
            tools:layout_constraintLeft_creator="1"
            card_view:layout_constraintHorizontal_bias="0.014" />

        <ImageView
            android:id="@+id/button12"
            android:layout_width="44dp"
            android:layout_height="41dp"
            android:paddingTop="5dp"
            android:textSize="22sp"
            android:layout_gravity="center"
            android:src="@drawable/college"
            android:onClick="onClick"
            android:textAlignment="center"
            android:textStyle="italic"
            android:layout_weight="0.10"
            card_view:layout_constraintTop_toBottomOf="@+id/button13"
            card_view:layout_constraintBottom_toBottomOf="@+id/button11"
            android:layout_marginBottom="2dp"
            card_view:layout_constraintVertical_bias="0.34"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="65dp"
            android:layout_marginEnd="65dp"
            card_view:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            card_view:layout_constraintLeft_toLeftOf="parent"
            card_view:layout_constraintHorizontal_bias="0.011" />

        <ImageView
            android:id="@+id/button13"
            android:layout_width="43dp"
            android:layout_height="38dp"
            android:paddingTop="5dp"
            android:textSize="22sp"
            android:layout_gravity="center"
            android:textAlignment="center"
            android:onClick="onClick"
            android:src="@drawable/alert"
            android:textStyle="italic"
            android:padding="5dp"
            android:layout_weight="0.10"
            android:layout_marginRight="65dp"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="65dp"
            card_view:layout_constraintHorizontal_bias="0.009"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="65dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="65dp"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="136dp" />

        <TextView
            android:id="@+id/tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button10"
            android:layout_toStartOf="@+id/button10"
            android:text="Match"
            card_view:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="229dp"
            tools:layout_constraintLeft_creator="1"
            card_view:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="169dp"
            android:layout_height="46dp"
            android:text="Home"
            android:textSize="20dp"
            android:onClick="onClick"
            android:textAlignment="center"
            android:textColor="#000"
            android:textStyle="bold"
            android:layout_marginTop="28dp"
            card_view:layout_constraintTop_toBottomOf="@+id/button9"
            android:layout_marginBottom="11dp"
            card_view:layout_constraintBottom_toTopOf="@+id/textView11"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintLeft_toLeftOf="parent"
            card_view:layout_constraintHorizontal_bias="0.502"
            card_view:layout_constraintVertical_bias="0.685"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="167dp"
            android:layout_height="39dp"
            android:onClick="onClick"
            android:text="Panic Alert"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textAlignment="center"
            android:textColor="#000"
            android:layout_marginBottom="30dp"
            card_view:layout_constraintBottom_toTopOf="@+id/textView12"
            card_view:layout_constraintRight_toRightOf="parent"
            card_view:layout_constraintLeft_toLeftOf="parent"
            card_view:layout_constraintHorizontal_bias="0.502"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <TextView
            android:id="@+id/textView12"
            android:textColor="#000"
            android:layout_width="170dp"
            android:textStyle="bold"
            android:layout_height="41dp"
            android:text="About Us"
            android:onClick="onClick"
            android:textSize="20dp"
            android:textAlignment="center"
            android:layout_marginBottom="20dp"
            card_view:layout_constraintBottom_toTopOf="@+id/textView13"
            card_view:layout_constraintLeft_toLeftOf="parent"
            card_view:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintLeft_creator="1" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="169dp"
            android:layout_height="38dp"
            android:textColor="#000"
            android:text="Exit"
            android:onClick="onClick"
            android:textStyle="bold"
            android:textSize="20dp"
            android:textAlignment="center"
            card_view:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="108dp"
            card_view:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="113dp"
            android:layout_marginBottom="6dp" />

    </android.support.constraint.ConstraintLayout>


</android.support.v4.widget.DrawerLayout>
